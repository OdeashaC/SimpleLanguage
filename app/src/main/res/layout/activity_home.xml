<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Home">

    <Button
        android:id="@+id/JapaneseBtn"
        android:layout_width="150dp"
        android:layout_height="58dp"
        android:layout_marginStart="134dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="127dp"
        android:layout_marginBottom="12dp"
        android:text="Japenese"
        app:layout_constraintBottom_toTopOf="@+id/FrenchBtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/SpanishBtn" />

    <Button
        android:id="@+id/KoreanBtn"
        android:layout_width="150dp"
        android:layout_height="58dp"
        android:layout_marginStart="134dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="127dp"
        android:layout_marginBottom="9dp"
        android:text="Korean"
        app:layout_constraintBottom_toTopOf="@+id/ThaiBtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/FrenchBtn" />

    <Button
        android:id="@+id/FrenchBtn"
        android:layout_width="150dp"
        android:layout_height="58dp"
        android:layout_marginStart="134dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="127dp"
        android:layout_marginBottom="4dp"
        android:text="French"
        app:layout_constraintBottom_toTopOf="@+id/KoreanBtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/JapaneseBtn" />

    <Button
        android:id="@+id/ThaiBtn"
        android:layout_width="150dp"
        android:layout_height="58dp"
        android:layout_marginStart="134dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="127dp"
        android:layout_marginBottom="69dp"
        android:text="Thai"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/KoreanBtn" />

    <Button
        android:id="@+id/SpanishBtn"
        android:layout_width="150dp"
        android:layout_height="58dp"
        android:layout_marginStart="130dp"
        android:layout_marginTop="323dp"
        android:layout_marginEnd="131dp"
        android:layout_marginBottom="8dp"
        android:text="Spanish"
        app:layout_constraintBottom_toTopOf="@+id/JapaneseBtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="307dp"
        android:layout_height="85dp"
        android:layout_marginStart="67dp"
        android:layout_marginTop="188dp"
        android:layout_marginEnd="37dp"
        android:layout_marginBottom="44dp"
        android:text="Hello World!"
        android:textAlignment="center"
        android:textSize="50dp"
        app:layout_constraintBottom_toTopOf="@+id/SpanishBtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>